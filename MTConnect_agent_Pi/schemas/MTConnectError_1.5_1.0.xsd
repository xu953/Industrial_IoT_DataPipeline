        <xs:annotation>
          <xs:documentation>
            Revolutions per minute
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='SECOND'>
        <xs:annotation>
          <xs:documentation>
            A measurement of time.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
